{
  "name": "express-react-redux",
  "description": "Express middleware for React/Redux applications",
  "version": "0.3.1",
  "repository": "dai-shi/express-react-redux",
  "main": "./lib/main.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "pretest": "npm run lint --silent",
    "test": "jest --forceExit",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "commander": "^2.11.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "json": "^9.0.6",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "require-from-string": "^1.2.1",
    "serialize-javascript": "^1.4.0",
    "serve-static": "^1.12.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
    "fs-extra": "^4.0.1",
    "jest": "^21.0.1",
    "request": "^2.81.0"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-param-reassign": 0,
      "global-require": 0,
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ],
      "strict": 0,
      "no-console": 0,
      "react/no-unused-prop-types": [
        2,
        {
          "skipShapeProps": true
        }
      ],
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "__PRELOADED_STATE__"
          ]
        }
      ]
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
