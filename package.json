{
  "name": "express-react-redux",
  "description": "Express middleware for React/Redux applications",
  "version": "0.5.0",
  "repository": "dai-shi/express-react-redux",
  "main": "./lib/main.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "pretest": "npm run lint --silent",
    "test": "jest --forceExit",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-react": "^7.0.0-beta.32",
    "@babel/preset-stage-3": "^7.0.0-beta.32",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.0",
    "commander": "^2.12.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "json": "^9.0.6",
    "lodash": "^4.17.4",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "require-from-string": "^2.0.1",
    "serialize-javascript": "^1.4.0",
    "serve-static": "^1.13.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "jest": "^21.2.1",
    "request": "^2.83.0"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-param-reassign": 0,
      "global-require": 0,
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ],
      "strict": 0,
      "no-console": 0,
      "react/no-unused-prop-types": [
        2,
        {
          "skipShapeProps": true
        }
      ],
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "__PRELOADED_STATE__"
          ]
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        2,
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ]
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
