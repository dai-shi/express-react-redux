{
  "name": "express-react-redux",
  "description": "Express middleware for React/Redux applications",
  "version": "0.1.1",
  "repository": "dai-shi/express-react-redux",
  "main": "./lib/main.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "pretest": "npm run lint --silent",
    "test": "jest --forceExit",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "commander": "^2.9.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json": "^9.0.4",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "serve-static": "^1.11.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.1",
    "fs-extra": "^2.0.0",
    "jest": "^18.1.0",
    "request": "^2.79.0"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-param-reassign": 0,
      "global-require": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "strict": 0,
      "no-console": 0,
      "react/no-unused-prop-types": [
        "error",
        {
          "skipShapeProps": true
        }
      ],
      "no-underscore-dangle": ["error", { "allow": ["__PRELOADED_STATE__"] }]
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
